{
  "mcpServers": {
    "apple-docs": {
      "command": "node",
      "args": [
        "/Users/sj/Dev/apple-docs-mcp-local/dist/index.js"
      ]
    },
    "github": {
      "command": "node",
      "args": [
        "/Users/sj/.npm-global/lib/node_modules/@modelcontextprotocol/server-github/dist/index.js"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "macos-automator": {
      "command": "node",
      "args": [
        "/Users/sj/.npm-global/lib/node_modules/@steipete/macos-automator-mcp/dist/server.js"
      ]
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "claude-code",
        "--project-from-cwd"
      ],
      "env": {
        "ENABLE_TOOL_SEARCH": "true"
      }
    },
    "openaiDeveloperDocs": {
      "type": "http",
      "url": "https://developers.openai.com/mcp"
    },
    "xcode": {
      "command": "xcrun",
      "args": [
        "mcpbridge"
      ]
    },
    "memory": {
      "command": "node",
      "args": [
        "/Users/sj/.npm-global/lib/node_modules/@modelcontextprotocol/server-memory/dist/index.js"
      ],
      "env": {
        "MEMORY_FILE_PATH": "/Users/sj/.claude/memory/knowledge-graph.jsonl"
      }
    },
    "nvidia-build": {
      "command": "/Users/sj/.local/share/nvidia-mcp-venv/bin/python3",
      "args": [
        "/Users/sj/.local/share/nvidia-mcp-venv/nvidia_mcp_server.py"
      ]
    }
  }
}
